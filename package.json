    "proptypes": "^1.1.0",
    "ramda": "^0.25.0",
    "re-base": "^3.2.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "redux": "^3.7.2",
