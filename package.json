{
  "name": "topspin-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "email-validator": "^1.1.1",
    "firebase": "^4.12.1",
    "grommet": "^1.10.1",
    "history": "^4.7.2",
    "moment": "^2.22.0",
